@page
@model webapp.Pages.Logowanie.SignUpModel
@{
    ViewData["Title"] = "Rejestracja";
    Layout = "../Shared/_LayoutNoone.cshtml";
}
<html>
<head>
 
    @Model.Msg
</head>
<body>

    <h3>Zarejestruj się</h3>
    <form method="post" asp-page="signup">
        <table>
            <tr>
                <td>Numer telefonu</td>
                <td>
                    <input asp-for="@Model.klient.nr_telefonu" />
                </td>
            </tr>
            <tr>
                <td>Hasło</td>
                <td>
                    <input asp-for="@Model.klient.haslo" />
                </td>
            </tr>
            <tr>
                <td>Imie</td>
                <td>
                    <input asp-for="@Model.klient.imie" />
                </td>
            </tr>
            <tr>
                <td>Nazwisko</td>
                <td>
                    <input asp-for="@Model.klient.nazwisko" />
                </td>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td>
                    <input type="submit" value="Zarejestruj" />
                    <br />
                    <a asp-page="/LogIn">Zaloguj się</a>
                </td>
            </tr>
        </table>
    </form>

</body>
</html>
